{
  "name": "react-pdf-monorepo",
  "version": "1.0.0",
  "description": "react-pdf monorepo",
  "type": "module",
  "workspaces": [
    "packages/*",
    "test"
  ],
  "scripts": {
    "build": "npm run build -w react-pdf",
    "dev": "npm run watch -w react-pdf && npm run dev -w test",
    "format": "npm run format",
    "lint": "npm run lint",
    "test": "npm run test",
    "tsc": "npm run tsc",
    "unit": "npm run unit"
  },
  "devDependencies": {
    "lint-staged": "^15.0.0",
    "prettier": "^3.2.0"
  },
  "resolutions": {
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.28.0"
  },
  "packageManager": "npm@10.2.3"
}
